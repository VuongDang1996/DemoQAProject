<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="CucumberTestSuite" verbose="1" parallel="methods" thread-count="3">

    <test name="SmokeTests">
        <classes>
            <class name="com.demoqa.cucumber.runners.SmokeTestRunner"/>
        </classes>
    </test>

    <test name="RegressionTests">
        <classes>
            <class name="com.demoqa.cucumber.runners.RegressionTestRunner"/>
        </classes>
    </test>

</suite>
